<h1>Suppliers</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Address</th>
  </tr>


  <% for supplier in @suppliers %>
  <tr>
    <td><a href="/suppliers/<%= supplier.id %>"><%= supplier.name %></a></td>
    <td><%= supplier.address %></td>
  </tr>
  <% end %>
  <a href="/books/new">Add New</a>

</table>
